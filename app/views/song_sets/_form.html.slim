= form_for @song_set do |f|
  - if @song_set.errors.any?
    ul
      - @song_set.errors.each do |msg|
        li= msg
  div.field
    = f.label :name
    = f.text_field :name
  div.field
    = f.label :play_on
    = f.text_field :play_on, class: :datepicker
  div.actions
    = f.submit :class => 'btn'
